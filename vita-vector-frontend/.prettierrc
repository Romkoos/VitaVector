{
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": true,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "@/components/(.*)$",
    "@/Layout/(.*)$",
    "@/ui/(.*)$",
    "@/providers/(.*)$",
    "@/constants/(.*)$",
    "@/types/(.*)$",
    "@/assets/(.*)$",
    "@/config/(.*)$",
    "@/store/(.*)$",
    "@/hooks/(.*)$",
    "@/utils/(.*)$",
    "@/api/(.*)$",
    "^(.*)",
    "^(.*)",
    "(.scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}